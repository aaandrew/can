{{> navbar}}
</br><center>
<div class="schoollogo">
	<img src="https://upload.wikimedia.org/wikipedia/commons/f/f6/UCSD_logo.png" alt="" height="75%" width="75%"> </div>
<h3> Click on a student if you want to ask them a question!</h3>
</center>
{{#each ucsdstudents}}
	<div class="student">
		<div id="stuPicture">
			<img src={{image}} alt="" width="100px">
		</div>

		<div class="studentname">
			<a href={{href}}> {{name}}</a>
		</div>
		<ul>
			<li> {{year}}</li>
			<li> {{major}}</li>
			<li> {{college}}</li>
			<li> {{fraternity}}</li>
			<li> {{extracurriculars}} </li>
			<li> {{housing}} </li>
		</ul>
	</div>
{{/each}}
